<html>
	<head>
		<title>{{product}} ({{release}}) - Documentation</title>
		<style>
			{{include "bootstrap.css"}}
			{{include "style.css"}}
		</style>
	</head>
	<body name="top">
		<nav>
			<ul>
				<li class="product sep"><a href="#top">{{product}} <span class="release">({{release}})</span></a></li>
				<li><a href="#about">About</a></li>
				<li><a href="#installation">Installation</a></li>
				<li class="sep"><a href="#example">Example</a></li>
				<li><strong>API Documentation</strong></li>
				{{#modules}}
				<li class="module sep">
					<a href="#{{name}}">{{name}}</a>
					<ul class="methods">
						{{#methods}}
						<li class="method"><a href="#{{name}}">{{name}}</a></li>
						{{/methods}}
					</ul>
				</li>
				{{/modules}}
				<li class="datetime"><small>Generated: {{datetime}} by <a
					href="https://github.com/theangryangel/dox-tpl">dox-tpl</a></small></li>
			</ul>
		</nav>

		<div class="container">

			<header>
				<div class="hero-unit">
					<h1>{{product}} <small>{{release}}</small></h1>
					<p>Documentation</p>
	    		</div>
			</header>

			{{include "about.handlebars"}}
		
			{{#modules}}
			<article id="{{name}}" class="module">
				<header>
					<h2>{{name}} <small>module</small></h2>
				</header>

				<section class="params">
					<dl class="dl-horizontal">
						{{#params}}
						<dt>param</dt> <dd>{{types}} {{name}} {{description}}</dd>
						{{/params}}

						{{#returns}}
						<dt>return</dt> <dd>{{types}} {{description}}</dd>
						{{/returns}}
					</dl>
				</section>

				<section>
					{{{description_full}}}
				</section>

				{{#methods}}
				<section class="method" id="{{name}}">
					<header>
						<h3>{{name}} <small>{{type}}</small></h3>
					</header>

					<section class="params">
						<dl class="dl-horizontal">
							{{#params}}
							<dt>param</dt> <dd>{{types}} {{name}} {{description}}</dd>
							{{/params}}

							{{#returns}}
							<dt>return</dt> <dd>{{types}} {{description}}</dd>
							{{/returns}}
						</dl>
					</section>

					<section>
						{{{description_full}}}
					</section>

				</section>
				{{/methods}}

				<section>
					{{example}}
				</section>
			</article>
			{{/modules}}

		</div>
	</body>
</html>
